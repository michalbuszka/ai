<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RuchyBrowna"
        xmlns:oxy="http://oxyplot.org/wpf" x:Class="RuchyBrowna.MainWindow"
        mc:Ignorable="d"
        Title="Proces Browna i Marcova" Height="900" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <oxy:PlotView Grid.Row="0" Model="{Binding marcovModel}" />
        <oxy:PlotView Grid.Row="1" Model="{Binding brownModel}" />
        <oxy:PlotView Grid.Row="2" Model="{Binding marcov2DModel}" />
    </Grid>
</Window>
